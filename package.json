{
  "name": "cranach-search",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "dev:preview": "vite --host --mode intern",
    "build": "tsc && vite build",
    "build:external": "tsc && vite build --base=/search/",
    "build:internal": "tsc && vite build --base=/intern/search/ --mode intern",
    "build:external:preview": "tsc && vite build --base=/intern/search-external-preview/",
    "build:internal:preview": "tsc && vite build --base=/intern/search-internal-preview/ --mode intern",
    "serve": "vite preview",
    "lint": "stylelint **/*.scss",
    "lint:fix": "stylelint **/*.scss --fix"
  },
  "dependencies": {
    "history": "^5.1.0",
    "i18next": "^20.4.0",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "nouislider": "^15.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "sass": "^1.38.0",
    "stylelint": "^16.14.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.0",
    "typescript": "^4.3.5",
    "vite": "^6.1.0"
  }
}
